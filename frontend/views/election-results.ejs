<style>
  .results-container {
    background-color: var(--bg-color);
    padding: 30px;
    border-radius: 10px;
    max-width: 800px;
    margin: 0 auto;
  }
  
  .back-button {
    margin-bottom: 20px;
  }
  
  .back-button a {
    display: inline-flex;
    align-items: center;
    color: var(--text-color);
    text-decoration: none;
    font-weight: 500;
  }
  
  .results-title {
    font-size: 24px;
    font-weight: 600;
    color: var(--primary-color);
    margin-bottom: 5px;
    text-align: left;
  }
  
  .election-name {
    font-size: 18px;
    font-weight: 500;
    color: var(--text-color);
    margin-bottom: 20px;
    text-align: left;
  }
  
  .download-button {
    margin-bottom: 30px;
  }
  
  .download-button a {
    display: inline-block;
    background-color: var(--accent-color);
    color: white;
    padding: 8px 16px;
    border-radius: 4px;
    text-decoration: none;
    font-weight: 500;
    font-size: 14px;
  }
  
  .results-section {
    margin-bottom: 30px;
    background-color: white;
    padding: 20px;
    border-radius: 8px;
    box-shadow: 0 2px 8px rgba(0, 0, 0, 0.05);
  }
  
  .results-section h3 {
    font-size: 18px;
    font-weight: 500;
    color: var(--primary-color);
    margin-bottom: 15px;
    text-align: left;
  }
  
  .race-results {
    display: flex;
    flex-direction: column;
    gap: 15px;
  }
  
  .candidate-result {
    display: flex;
    align-items: center;
    justify-content: space-between;
  }
  
  .candidate-name {
    font-size: 16px;
    font-weight: 500;
    width: 200px;
    color: var(--text-color);
  }
  
  .progress-container {
    flex: 1;
    height: 24px;
    background-color: #f0f0f0;
    border-radius: 4px;
    position: relative;
    display: flex;
    align-items: center;
  }
  
  .progress-bar {
    height: 100%;
    background-color: var(--accent-color);
    border-radius: 4px;
  }
  
  .percentage {
    position: absolute;
    right: 10px;
    font-weight: 500;
    color: #333;
  }
</style>

